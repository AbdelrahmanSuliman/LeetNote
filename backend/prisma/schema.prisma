datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model problem {
  id             Int      @id @default(autoincrement())
  code           String
  title          String
  explanation    String
  timeComplexity String
  spaceComplexity String
  optimization   String
  createdAt      DateTime @default(now())
  language       String
}
